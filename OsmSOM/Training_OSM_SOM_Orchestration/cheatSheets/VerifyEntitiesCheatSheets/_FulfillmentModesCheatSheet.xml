<?xml version="1.0" encoding="UTF-8"?>
   <cheatsheet
      title="Verify Fulfillment Modes">
  <intro>
      <description>
         This cheat sheet guides you through the fulfillment modes created by the design pattern.
      </description>
   </intro>
    <item
         title="Verify Fulfillment Modes: SOM_Deliver, SOM_Cancel">
      <description>
		The fulfillment modes have been created with <b>Namespace</b> and <b>Description</b> set according to the entries in the design pattern.
	 </description>
      <subitem
            label="Open Fulfillment Mode: SOM_Deliver"
            skip="true">
         <command
               required="false"
               serialization="org.eclipse.ui.navigate.openResource(filePath=/Training_OSM_SOM_Orchestration/model/SOM_Deliver.fulfillmentMode)"/>
      </subitem>
      <subitem
            label="Open Fulfillment Mode: SOM_Cancel"
            skip="true">
         <command
               required="false"
               serialization="org.eclipse.ui.navigate.openResource(filePath=/Training_OSM_SOM_Orchestration/model/SOM_Cancel.fulfillmentMode)"/>
      </subitem>
   </item>
  </cheatsheet>
