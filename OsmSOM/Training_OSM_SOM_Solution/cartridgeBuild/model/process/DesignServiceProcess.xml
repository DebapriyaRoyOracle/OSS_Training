<?xml version="1.0" encoding="UTF-8"?>
<process xmlns="http://www.metasolv.com/OMS/OrderModel/2002/06/25" name="DesignServiceProcess" workstream="false">
  <description>DesignServiceProcess</description>
  <expectedDuration>P0Y0M0DT0H0M86400S</expectedDuration>
  <startActivity>CaptureBITask</startActivity>
  <activity name="CaptureBITask">
    <task>CaptureBITask</task>
    <description>CaptureBITask</description>
    <inputType>ANY</inputType>
    <processHistorySummary>false</processHistorySummary>
  </activity>
  <activity name="ProcessBITask">
    <task>ProcessBITask</task>
    <description>ProcessBITask</description>
    <inputType>ANY</inputType>
    <processHistorySummary>false</processHistorySummary>
  </activity>
  <activity name="ApproveBITask">
    <task>ApproveBITask</task>
    <description>ApproveBITask</description>
    <inputType>ANY</inputType>
    <processHistorySummary>false</processHistorySummary>
  </activity>
  <transition>
    <from>
      <activity>ProcessBITask</activity>
      <status>success</status>
    </from>
    <to>ApproveBITask</to>
    <mandatoryCheck>false</mandatoryCheck>
  </transition>
  <transition>
    <from>
      <activity>CaptureBITask</activity>
      <status>success</status>
    </from>
    <to>ProcessBITask</to>
    <mandatoryCheck>false</mandatoryCheck>
  </transition>
</process>