<?xml version="1.0" encoding="UTF-8"?>
<model:transformationSequence xmlns:model="http://xmlns.oracle.com/communications/ordermanagement/model" name="COM_OTM_Sequence" namespace="COM_SalesOrderFulfillment">
  <model:description>COM_OTM_Sequence</model:description>
  <model:targetOrderItemSpec>
    <model:name>COM_TransformedOrderLine</model:name>
    <model:namespace>COM_SalesOrderFulfillment</model:namespace>
  </model:targetOrderItemSpec>
  <model:transformationStage name="ProcessPrimaryRelationshipsStage">
    <model:orderItemContextPropertySelector value="true">ServiceInstanceIndicator</model:orderItemContextPropertySelector>
    <model:relationshipHandler>
      <model:relationshipType>
        <model:name>Primary</model:name>
        <model:namespace>OracleComms_Model_Base/1.0.0.0.0</model:namespace>
      </model:relationshipType>
      <model:implementation>oracle.communications.ordermanagement.orchestration.transformation.sequence.PrimaryRelationshipHandler</model:implementation>
    </model:relationshipHandler>
    <model:allowOverrides>false</model:allowOverrides>
  </model:transformationStage>
  <model:transformationStage name="ProcessChildrenRelationshipsStage">
    <model:usePreviousOrderItemContextSelector>true</model:usePreviousOrderItemContextSelector>
    <model:relatedOrderItemHierarchySelector direction="descendants">parentChildHierarchy</model:relatedOrderItemHierarchySelector>
    <model:relationshipHandler>
      <model:relationshipType>
        <model:name>Auxiliary</model:name>
        <model:namespace>OracleComms_Model_Base/1.0.0.0.0</model:namespace>
      </model:relationshipType>
      <model:implementation>oracle.communications.ordermanagement.orchestration.transformation.sequence.AuxiliaryRelationshipHandler</model:implementation>
    </model:relationshipHandler>
    <model:allowOverrides>true</model:allowOverrides>
  </model:transformationStage>
  <model:transition>
    <model:from>ProcessPrimaryRelationshipsStage</model:from>
    <model:to>ProcessChildrenRelationshipsStage</model:to>
  </model:transition>
</model:transformationSequence>