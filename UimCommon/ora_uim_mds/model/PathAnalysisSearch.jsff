                                                                                                   triggerType="dataChange"/>
                                                                                    <dvt:clientLayout name="forceDirectedLayout"
                                                                                                      method="DemoEditorLayout.forceDirectedLayout"
                                                                                                      featureName="DemoEditorLayout">
                                                                                        <f:attribute name="optimalLinkLength"
                                                                                                     value="200"/>
                                                                                        <f:attribute name="setViewport"
                                                                                                     value="true"/>
                                                                                    </dvt:clientLayout>
                                                                                    <dvt:clientLayout name="gridLayout"
                                                                                                          method="DemoGridLayout.gridLayout"
                                                                                                          featureName="DemoGridLayout"/>
                                                                                    <dvt:clientLayout name="circleLayout"
                                                                                                          method="DemoCircleLayout.circleLayout"
                                                                                                          featureName="DemoCircleLayout">
                                                                                            <f:attribute name="center"
                                                                                                         value="false"/>
                                                                                    </dvt:clientLayout>
                                                                                    <dvt:overviewWindow id="ow1"
                                                                                                        disclosed="true"/>
