                                                                                                       value="#{pageFlowScope.PathAnalysisDiagramBean.nodesModel}"
                                                                                                      var="node"
                                                                                                      rowDisclosureListener="#{pageFlowScope.PathAnalysisDiagramBean.disclosedNode}"
                                                                                                      selectionListener="#{pageFlowScope.PathAnalysisDiagramBean.nodesSelectedEdit}"
                                                                                                      selectedRowKeys="#{pageFlowScope.PathAnalysisDiagramBean.selectedNodes}"
                                                                                                      binding="#{pageFlowScope.PathAnalysisDiagramBean.diagramNodes}">
                                                                                        <af:dragSource actions="COPY LINK MOVE"
                                                                                                       defaultAction="MOVE"
                                                                                                       discriminant="fromDiagram"/>
                                                                                        <dvt:diagramNode id="nstep"
                                                                                                         nodeId="#{node.Nodeid}"
                                                                                                         draggable="yes"
                                                                                                         layout="forceDirectedLayout"
                                                                                                         showNodeActions="true"
                                                                                                         readOnly="true"
                                                                                                         showDrill="true"
                                                                                                         containerId="#{node.Containerid}"
                                                                                                         containerStyle="border-width:1px;border-color:black;"
                                                                                                         shortDesc="#{node.Label}">
                                                                                            <f:facet name="zoom100">
                                                                                                <af:panelGroupLayout layout="horizontal">
                                                                                                    <af:image source="#{node.Icon}"/>
                                                                                                    <af:outputText inlineStyle="font-size:16px;font-weight:bold;color:#333333"
                                                                                                                   value="#{node.Label}"/>
                                                                                                </af:panelGroupLayout>
                                                                                            </f:facet>
                                                                                        </dvt:diagramNode>
                                                                                    </dvt:diagramNodes>
                                                                                    <dvt:diagramLinks id="dl1"
                                                                                                      value="#{pageFlowScope.PathAnalysisDiagramBean.linksModel}"
                                                                                                      var="link"
                                                                                                      linkCreateListener="#{pageFlowScope.PathAnalysisDiagramBean.linkCreateListener}"
                                                                                                      linkChangeListener="#{pageFlowScope.PathAnalysisDiagramBean.linkUpdateListener}"
                                                                                                      selectionListener="#{pageFlowScope.PathAnalysisDiagramBean.linksSelected}"
                                                                                                      selectedRowKeys="#{pageFlowScope.PathAnalysisDiagramBean.selectedLinks}"
                                                                                                      binding="#{pageFlowScope.PathAnalysisDiagramBean.diagramLinks}">
                                                                                        <dvt:diagramLink id="l6"
                                                                                                         startNode="#{link.Startnode}"
                                                                                                         rendered="#{link.EntityType =='Connectivity'}"
                                                                                                         endNode="#{link.Endnode}"
                                                                                                         readOnly="false"
                                                                                                         startConnectorType="none"
                                                                                                         shortDesc="#{link.Label}"
                                                                                                         endConnectorType="none"
                                                                                                         linkStyle="dash"
                                                                                                         linkWidth="1"
                                                                                                         linkColor="black"/>
                                                                                        <dvt:diagramLink id="l7"
                                                                                                         startNode="#{link.Startnode}"
                                                                                                         rendered="#{link.EntityType =='Pipe'}"
                                                                                                         endNode="#{link.Endnode}"
                                                                                                         readOnly="false"
                                                                                                         startConnectorType="none"
                                                                                                         shortDesc="#{link.Label}"
                                                                                                         endConnectorType="none"
                                                                                                         linkStyle="solid"
                                                                                                         linkWidth="1"
                                                                                                         linkColor="black"/>
                                                                                    </dvt:diagramLinks>
                                                                                    <f:facet name="contextMenu">
                                                                                        <af:popup contentDelivery="lazyUncached"
                                                                                                  id="nodeMenu">
                                                                                            <af:menu id="m1">
                                                                                                <af:commandMenuItem text="#{inventoryUIBundle.VIEW_SUMMARY}"
                                                                                                                    id="cmi1"
                                                                                                                    action="#{pageFlowScope.PathAnalysisDiagramBean.nodeSummaryAction}"
                                                                                                                    actionListener="#{pageFlowScope.PathAnalysisDiagramBean.navigateToSummary}"/>
                                                                                            </af:menu>
                                                                                        </af:popup>
                                                                                    </f:facet>
                                                                                    <f:facet name="linkContextMenu">
                                                                                        <af:popup contentDelivery="lazyUncached"
                                                                                                  id="linkMenu">
                                                                                            <af:menu id="m2">
                                                                                                <af:commandMenuItem text="#{inventoryUIBundle.VIEW_SUMMARY}"
                                                                                                                    id="cmi4"
                                                                                                                    action="#{pageFlowScope.PathAnalysisDiagramBean.linkSummaryAction}"
                                                                                                                    actionListener="#{pageFlowScope.PathAnalysisDiagramBean.navigateToSummary}"/>
                                                                                            </af:menu>
                                                                                        </af:popup>
                                                                                    </f:facet>
                                                                                    <af:clientListener method="handleClick"
                                                                                                       type="click"/>
                                                                                </dvt:diagram>
                                                                            </f:facet>
                                                                            <f:facet name="end"></f:facet>
                                                                        </af:panelStretchLayout>
                                                                    </f:facet>
