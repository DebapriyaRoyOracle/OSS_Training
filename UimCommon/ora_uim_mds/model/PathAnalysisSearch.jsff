                                                                                                    <af:outputText value="#{pageFlowScope.PathAnalysisDiagramBean.id}"
                                                                                                                   id="ot30"/>
                                                                                                </af:link>
                                                                                            </af:panelLabelAndMessage>
                                                                                            <af:panelLabelAndMessage label="#{inventoryUIBundle.NAME}"
                                                                                                                     id="ntwk-name">
                                                                                                <af:outputText value="#{pageFlowScope.PathAnalysisDiagramBean.name}"
                                                                                                               id="ot20"/>
                                                                                            </af:panelLabelAndMessage>
                                                                                            <af:panelLabelAndMessage label="#{inventoryUIBundle.NETWORK_ENTITY_LOCATION}"
                                                                                                                     id="logdev-nel"
                                                                                                                     rendered="#{pageFlowScope.PathAnalysisDiagramBean.renderLocation}">
                                                                                                <af:link id="ln1"
                                                                                                         action="LocationDetails"
                                                                                                         actionListener="#{pageFlowScope.PathAnalysisDiagramBean.navigateToSummary}">
                                                                                                    <af:outputText value="#{pageFlowScope.PathAnalysisDiagramBean.location}"
                                                                                                                   id="ot50"/>
                                                                                                </af:link>
                                                                                            </af:panelLabelAndMessage>
                                                                                            <af:spacer height="10"/>
                                                                                            <af:iterator id="it123"
                                                                                                         binding="#{backingBeanScope.characteristicsBinding}"
