                                                                                           disabledIcon="/images/diagram/viewtreecircle_dis.png"
                                                                                           hoverIcon="/images/diagram/viewtreecircle_dwn.png"/>
                                                                                <af:spacer width="5"/>
                                                                                <af:button id="pasb4"
                                                                                           actionListener="#{pageFlowScope.PathAnalysisDiagramBean.layoutListener}"
                                                                                           shortDesc="#{inventoryUIBundle.GRID_LAYOUT}"
                                                                                           icon="/images/diagram/viewtree_horizontal_ena.png"
                                                                                           disabledIcon="/images/diagram/viewtree_horizontal_dis.png"
                                                                                           hoverIcon="/images/diagram/viewtree_horizontal_dwn.png"/>
                                                                            </af:toolbar>
                                                                        </af:group>
                                                                    </af:toolbox>
                                                                </af:panelGroupLayout>
