                                                                                               dimensionsFrom="auto">
                                                                                <af:showDetailItem disclosed="true"
                                                                                                   id="sdiEditor"
                                                                                                   text="Editor"
                                                                                                   flex="1"
                                                                                                   stretchChildren="first">
                                                                                    <af:panelGroupLayout id="pgl30"
                                                                                                         layout="scroll">
                                                                                        <af:panelFormLayout id="pfl10"
                                                                                                            maxColumns="1"
                                                                                                            labelWidth="33%"
                                                                                                            fieldWidth="67%"
                                                                                                            binding="#{backingBeanScope.viewPanel}">
                                                                                            <af:panelLabelAndMessage label="#{inventoryUIBundle.ID}"
                                                                                                                     id="ntwk-id">
                                                                                                <af:link id="ln2"
                                                                                                         action="#{pageFlowScope.PathAnalysisDiagramBean.nodeSummaryAction}"
