										  summary="#{inventoryUIBundle.CAPACITY_CONSUMED_TABLE}"> 
									<af:column headerText="#{inventoryUIBundle.ID}"
											   id="c21" rowHeader="unstyled">
										<af:commandImageLink id="cl4" action="init" actionListener="#{bindings.openSubTaskForPipeSummary.execute}"
															 text="#{row.id}"
															 shortDesc="#{inventoryUIBundle.LINK_FOR_SUMMARY}">
										   
											<af:setActionListener from="#{row.oid}" to="#{pageFlowScope.objectId}"/>
										</af:commandImageLink>
									</af:column>
									
									<af:column headerText="#{inventoryUIBundle.NAME}"
											   id="c19">
										<af:outputText value="#{row.name}" id="ot27"/>
									</af:column>
									
									<af:column sortable="false" headerText="#{inventoryUIBundle.SPECIFICATION}" id="c7">
										<af:commandImageLink action="SpecificationSummary" id="cl5" actionListener="#{bindings.openSubTaskForSpec.execute}"
															 text="#{row.specification.name}"
															 shortDesc="#{inventoryUIBundle.LINK_FOR_SPECIFICATION_SUMMARY}">
										   
											<af:setActionListener from="#{row.specification.oid}" to="#{specObjectId}"/>
										</af:commandImageLink>
									</af:column>
									<af:column headerText="#{inventoryUIBundle.INVENTORY_STATUS}"
										 id="c9">
										<af:outputText value="#{pageFlowScope.PipeSummaryBean.childInventoryStatus}"
											   id="ot31"/>
									</af:column>
									<af:column headerText="#{inventoryUIBundle.CONSUMPTION_PERCENTAGE}"
											   id="c27">
										<af:outputText value="#{pageFlowScope.PipeSummaryBean.consumption}" id="ot29"/>
									</af:column>
									<af:column headerText="#{inventoryUIBundle.CAPACITY_CONSUMED}"
											   id="c2811">
										<af:outputText value="#{pageFlowScope.PipeSummaryBean.capacityConsumed}" id="ot3011"/>
									</af:column>
								</af:table>
							</af:panelCollection>
						 </af:showDetailHeader>
					 </af:showDetailItem>
					 
					<af:showDetailItem text="#{inventoryUIBundle.SUFFIX_HIERARCHY}" id="HierarchyTab" styleClass="AFStretchWidth" binding="#{backingBeanScope.pipe_hierarchyTab}" 
                        rendered="#{pageFlowScope.PipeSummaryBean.showTree and !pageFlowScope.PipeSummaryBean.disablePipeHierarchyPortlet}"
						disclosureListener="#{pageFlowScope.PipeSummaryBean.discloseTab}" 
                        disclosed="#{pageFlowScope.PipeSummaryBean.selectedTab == inventoryUIBundle.SUFFIX_HIERARCHY}">
						<!-- PIPE HIERARCHY -->
					  <!--Pipe Tree Starts -->					  
					  <af:showDetailHeader text="#{inventoryUIBundle.PIPE_HIERARCHY}" visible="#{pageFlowScope.PipeSummaryBean.showTree and !pageFlowScope.PipeSummaryBean.disablePipeHierarchyPortlet and 
					   pageFlowScope.PipeSummaryBean.selectedTab == inventoryUIBundle.SUFFIX_HIERARCHY}"
					   disclosed="true" id="sdh31" binding="#{backingBeanScope.pipeTreeShowDetailHeader}"
					   styleClass="invSummaryShowDetailHeader">
						<f:facet name="context"/>
						<f:facet name="menuBar"/>
						<f:facet name="toolbar"/>
						<f:facet name="legend"/>
						<f:facet name="info"/>
						<af:panelCollection id="pc21"
											styleClass="AFStretchWidth" rendered="#{pageFlowScope.PipeSummaryBean.showTree}">
						  <f:facet name="menus"></f:facet>
							  <f:facet name="toolbar">

							  </f:facet>
						  <f:facet name="statusbar"/>
						  <af:treeTable value="#{pageFlowScope.PipeTreeBean.treeModel}"
										var="node" rowSelection="single"
										id="pipeHierarchyTreeTable"
										fetchSize="#{pageFlowScope.PipeTreeBean.fetchSize}"
										contextMenuSelect="true" styleClass="AFStretchWidth"
										contentDelivery="immediate" expandAllEnabled="#{pageFlowScope.PipeTreeBean.expandAllEnabled}"
										selectionListener="#{pageFlowScope.PipeTreeBean.nodeSelectedEvent}"
										binding="#{backingBeanScope.PIPE_treeBinding}"
										rowDisclosureListener="#{pageFlowScope.PipeTreeBean.rowDisclosureListener}"
										disclosedRowKeys="#{pageFlowScope.PipeTreeBean.treeDisclosedRowKeys}"
										rowBandingInterval="0"
										horizontalGridVisible="false" 
										summary="#{inventoryUIBundle.PIPE_HIERARCHY_TREETABLE}">
							<f:facet name="nodeStamp">
							  <af:column sortable="false" id="column2" width="500"
										 headerText="#{inventoryUIBundle.PIPE}" rowHeader="unstyled">
								<af:panelGroupLayout id="pgl5">
								  <af:image source="#{node.data.icon}" id="nodeIcon"
											styleClass="invSummaryImage"
											shortDesc="icon"/>
								  <af:switcher id="idSwitcher" defaultFacet="false" facetName="#{pageFlowScope.PipeTreeBean.disableNodeLink}">
									<f:facet name="false">
									  <af:commandImageLink action="#{pageFlowScope.PipeTreeBean.view}"
													  id="cl3" actionListener="#{pageFlowScope.PipeTreeBean.registerRecentItem}"
														   text="#{node.data.label}"
														   shortDesc="#{inventoryUIBundle.LINK_FOR_NAVIGATION}">
									
									  </af:commandImageLink>
									</f:facet>
									<f:facet name="true">
									  <af:outputText value="#{node.data.label}" id="ot2"/>
									</f:facet>
								  </af:switcher>
								</af:panelGroupLayout>
							  </af:column>
							</f:facet>
							
							<af:column sortable="false" headerText="#{inventoryUIBundle.CONDITION}" id="column5" filterable="false">
							  <af:outputText value="#{node.data.condition}" id="ot315"/>
							</af:column>
							
							
							<af:column sortable="false" headerText="#{inventoryUIBundle.INVENTORY_STATUS}" id="column6" filterable="false">
							  <af:outputText value="#{node.data.invStatus}" id="ot313"/>
							</af:column>
							  
							<af:column sortable="false" headerText="#{inventoryUIBundle.RESOURCE_ASSIGNMENT_STATUS}" id="column7" filterable="false">
							  <af:outputText value="#{node.data.raStatus}" id="ot324"/>
							</af:column>    
							
							<af:column sortable="false" headerText="#{inventoryUIBundle.CONSUMERS}" id="colun3" filterable="false">
							   <af:iterator id="it123" value="#{node.data.consumers}" var="consumer">
								 <af:link actionListener="#{pageFlowScope.PipeTreeBean.viewConsumer}" action="#{pageFlowScope.PipeTreeBean.getConsumerAction}" id="cn16"  disabled="#{pageFlowScope.PipeTreeBean.BISwitchNotPossible}">
									<af:outputText value="#{consumer.label}" id="onn312" binding="#{pageFlowScope.consumer}"/>
									<f:param name="consumerOid" value="#{consumer.object.oid}" id="par222"/>
								 </af:link>
									<af:outputText value=" (#{consumer.invStatus})" id="ot14"/>
								 <br></br>
							   </af:iterator>  
							</af:column>
							
						  </af:treeTable>
						   </af:panelCollection>
						</af:showDetailHeader>
					  <!--Pipe Tree Ends -->
					</af:showDetailItem>
					
					<af:showDetailItem text="#{inventoryUIBundle.ASSOCIATED_RESOURCES}" id="AssocResourcesTab" styleClass="AFStretchWidth" binding="#{backingBeanScope.pipe_resourcesTab}"  
					rendered="#{pageFlowScope.PipeSummaryBean.renderAssociatedResourcesTab}" disclosureListener="#{pageFlowScope.PipeSummaryBean.discloseTab}" 
					disclosed="#{pageFlowScope.PipeSummaryBean.selectedTab == inventoryUIBundle.ASSOCIATED_RESOURCES}">						
						<!-- CUSTOM INVOLVEMENT -->													
						<af:region value="#{bindings.CustomInvolvementListFlow1.regionModel}" id="r7"/>											
						<af:spacer width="10" height="10"/>
						<!-- NETWORK EDGES -->
						<af:region value="#{bindings.NetworkEdgeListFlow1.regionModel}" id="r8"/>
					</af:showDetailItem>
					
					<af:showDetailItem text="#{inventoryUIBundle.CONSUMERS}" id="AssignmentsTab" styleClass="AFStretchWidth" binding="#{backingBeanScope.pipe_consumersTab}"  
					rendered="#{pageFlowScope.PipeSummaryBean.renderConsumersTab}" disclosureListener="#{pageFlowScope.PipeSummaryBean.discloseTab}" 
					disclosed="#{pageFlowScope.PipeSummaryBean.selectedTab == inventoryUIBundle.CONSUMERS}">
						<!-- ASSIGNMENTS -->
						<af:region value="#{bindings.ConsumerListFlow1.regionModel}" id="r9"/>
						<af:spacer width="10" height="10"/>
						<!-- REFERENCES -->
						<af:region value="#{bindings.ReferenceListFlow1.regionModel}" id="r10"/>
						<af:spacer width="10" height="10"/>
						<!-- BUSINESS INTERACTIONS -->
						<af:region value="#{bindings.BusinessInteractionListFlow1.regionModel}" id="r3"/>
					</af:showDetailItem>
					
					<af:showDetailItem text="#{inventoryUIBundle.GROUPS_AND_INFRASTRUCTURE}" id="GroupsTab" styleClass="AFStretchWidth" binding="#{backingBeanScope.pipe_groupInfraTab}"  
					rendered="#{pageFlowScope.PipeSummaryBean.renderGroupsTab}" disclosureListener="#{pageFlowScope.PipeSummaryBean.discloseTab}" 
					disclosed="#{pageFlowScope.PipeSummaryBean.selectedTab == inventoryUIBundle.GROUPS_AND_INFRASTRUCTURE}">
						<!-- PLACES -->
						<af:region value="#{bindings.PlaceListFlow2.regionModel}" id="r2"/>
						<af:spacer width="10" height="10"/>
						<!-- INVENTORY GROUP -->
						<af:region value="#{bindings.InventoryGroupListFlow1.regionModel}" id="r1"/>
						<af:spacer width="10" height="10"/>
						<!-- INVENTORY ROLES -->
						<af:region value="#{bindings.InventoryRoleListFlow1.regionModel}" id="r5"/>
					</af:showDetailItem>
					
				</af:panelTabbed>
			</af:panelGroupLayout>
        </af:panelHeader>
      </f:facet>
      </af:panelStretchLayout>
    </f:facet>
  </af:pageTemplate>
</jsp:root>
