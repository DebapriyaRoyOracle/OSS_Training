									  var="row" rows="#{pageFlowScope.PipeSummaryBean.trailSize}" autoHeightRows="#{pageFlowScope.PipeSummaryBean.portletHeight}" contentDelivery="immediate" 
									  emptyText="#{inventoryUIBundle.TABLE_EMPTY_TEXT_NO_ROWS_YET}"
									  fetchSize="#{pageFlowScope.PipeSummaryBean.fetchSize}"
									  rowBandingInterval="0"
									  rowSelection="#{pageFlowScope.PipeSummaryBean.rowSelection}" width="100%"
