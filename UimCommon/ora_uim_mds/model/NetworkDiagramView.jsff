                                                            <af:resource type="javascript">
                                                              function handleClick(event) {
                                                                  event.cancel();
                                                              }

                                                              var bMouseDragging = false;
                                                              var nMouseOffsetX = 0;
                                                              var nMouseOffsetY = 0;

                                                              function mouseDown(evt) {
                                                                  bMouseDragging = true;
                                                                  console.log("mouseDown" + bMouseDragging);
                                                                  var ball = document.getElementById("N1");
                                                                  console.log("mouseDown-Node" + ball);
                                                                  var p = document.documentElement.createSVGPoint();
                                                                  console.log("mouseDown-SVG Point" + p);
                                                                  p.x = evt.clientX;
                                                                  p.y = evt.clientY;
                                                                  console.log("mouseDown- (x,y)" + p.x + "--" + p.y);
                                                                  var m = ball.getScreenCTM();
                                                                  console.log("mouseDown-ScreenCTM" + m);
                                                                  p = p.matrixTransform(m.inverse());
                                                                  console.log("mouseDown-matrixTransform" + p);
                                                                  nMouseOffsetX = p.x - parseInt(ball.getAttribute("cx"));
                                                                  nMouseOffsetY = p.y - parseInt(ball.getAttribute("cy"));
                                                                  console.log("mouseDown- Mouse Offset" + nMouseOffsetX + "--" + nMouseOffsetY);
                                                              }

                                                              function mouseUp(evt) {
                                                                  bMouseDragging = false;
                                                                  console.log("mouseUp" + bMouseDragging);
                                                              }

                                                              function mouseMove(evt) {
                                                                  bMouseDragging = true;
                                                                  console.log("mouseMove is executing--" + bMouseDragging);
                                                                  var p = document.documentElement.createSVGPoint();
                                                                  console.log("mouseMove-SVG Point" + p);
                                                                  p.x = evt.clientX;
                                                                  p.y = evt.clientY;
                                                                  console.log("mouseMove- (x,y)" + p.x + "--" + p.y);
                                                                  var bClient = true;
                                                                  console.log("mouseMove" + bMouseDragging);
                                                                  var ball = document.getElementById("N1");
                                                                  console.log("mouseMove-------Node found----------" + ball);
                                                                  if (ball) {
                                                                      console.log("mouseMove--------------ball" + bMouseDragging);
                                                                      var m = ball.getScreenCTM();
                                                                      console.log("mouseMove-ScreenCTM" + m);
                                                                      p = p.matrixTransform(m.inverse());
                                                                      console.log("mouseMove-matrixTransform" + p);
                                                                      ball.setAttribute("cx", p.x - nMouseOffsetX);
                                                                      ball.setAttribute("cy", p.y - nMouseOffsetY);
                                                                      console.log("mouseMove- Mouse Offset" + ball.getAttribute("cx") + "--" + ball.getAttribute("cy"));
                                                                      bClient = false;
                                                                  }
                                                              }

                                                              function init() {
                                                                  var ball = document.getElementById("N1");
                                                                  console.log("init-------------" + ball);
                                                                  if (ball) {
                                                                      ball.addEventListener("mousedown", mouseDown, false);
                                                                      ball.addEventListener("mouseup", mouseUp, false);
                                                                      ball.addEventListener("mousemove", mouseMove, false);
                                                                  }
                                                              }
                                                            </af:resource>
